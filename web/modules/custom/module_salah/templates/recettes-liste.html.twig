
{{ attach_library('module_salah/salah-style') }}
<h4  class="animate__animated animate__bounce animate__repeat-2  salah">{{ title }}</h4>
<ol>
  {% for item in items %}
    <li class="animate__animated {{ random (['animate__bounce']) }}">
      {{ item.name }}
    </li>
  {% endfor %}
</ol>
<b style="color: #0a6fb4">NB: cela vient de notre fichier custum twig: recettes-liste.html.twig</b>
